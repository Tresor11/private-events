<br>
<div class="row justify-content-center">
<h1>List of Events</h1>
</div>
<br>
<br>

<div class="row justify-content-center">
    <div class="col-4">
        <h5>Upcoming Events:</h5>
        <ol>
          <%@upcoming.each do |x|%>
            <li><%= link_to  "#{x.title}", x%></li>
          <%end%>
        </ol>
    </div>
    <div class="col-4">
        <h5>Past events:</h5>
        <ol>
          <%@past.each do |x|%>
            <li><%= link_to  "#{x.title}", x%></li>
          <%end%>
        </ol>
    </div>
</div>